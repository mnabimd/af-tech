{
   "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "checkJs": false,
      "allowJs": true,
      "skipLibCheck": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
         {
            "name": "next"
         }
      ],
      "paths": {
         "@/*": ["./*"]
      },
      "strict": false
   },
   "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
   "exclude": ["node_modules"],
   "noImplicitAny": false,
   "strict": false
}
